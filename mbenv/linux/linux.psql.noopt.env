setenv DICOM_ROOT `pwd`
setenv DICOM_BIN /usr/local/dicom/bin
setenv DICOM_LIB $DICOM_ROOT/lib/linux.psql.noopt
setenv DICOM_LIBSRC $DICOM_ROOT/libsrc
setenv DICOM_INCLUDE $DICOM_ROOT/include 
setenv DICOM_MAKE $DICOM_ROOT/mbenv/linux/ctnmake.linux.psql.noopt

setenv CTN_ROOT /opt/ctn
setenv CGIROOT $DICOM_ROOT/external/cgihtml-1.69
setenv CGI_DIR /home/httpd/cgi-bin
setenv HTDOCS_DIR /home/httpd/html/mb

# These are the standard install, but standard does not
# support multi-byte characters.
#setenv PSQL_LIB /usr/lib
#setenv PSQL_INCLUDE /usr/include/pgsql

setenv PSQL_LIB /opt/postgres/lib
setenv PSQL_INCLUDE /opt/postgres/include

setenv ARCHITECTURE LITTLE_ENDIAN_ARCHITECTURE
setenv OS LINUX
setenv CC gcc
setenv XCC gcc
setenv DBLINK gcc
setenv INSTALL install
setenv INSTALLUID "install -m 4775"
setenv INSTALLINCLUDE "install -c -m 444 "
setenv CI "echo Use ci on a sun"
setenv CO "echo Use co on a sun"
setenv INDENT "Use indent on a sun"
setenv LINTOPTIONS $DICOM_ROOT/lint/minimal.lnt
setenv RANLIB ranlib
setenv LEX flex
