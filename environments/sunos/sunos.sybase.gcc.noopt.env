setenv CTN_TARGET /usr/local/ctn
setenv DICOM_ROOT `pwd`
setenv DICOM_BIN $CTN_TARGET/bin
setenv DICOM_LIB $DICOM_ROOT/lib/sunos.sybase.gcc.noopt
setenv DICOM_INCLUDE $DICOM_ROOT/include 
setenv DICOM_MAKE $DICOM_ROOT/environments/sunos/make.sunos.sybase.gcc.noopt
setenv ARCHITECTURE BIG_ENDIAN_ARCHITECTURE
setenv OS SUNOS
setenv CC gcc
setenv XCC gcc
setenv DBLINK gcc
#setenv INSTALL "install -s"
setenv INSTALL install
setenv INSTALLUID "install -m 4775"
setenv INSTALLINCLUDE "install -c -m 444 "
setenv CI ci
setenv CO "co -l"
setenv INDENT "indent -i4 -nfc1 -di4 -nip -bc"
setenv LINTOPTIONS $DICOM_ROOT/lint/minimal.lnt
setenv RANLIB ranlib
setenv LEX lex
